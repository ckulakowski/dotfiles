[alias]
  br = branch
  ci = commit
  co = checkout
  commits = git shortlog -s -n
  df = diff
  files = diff --name-only master...
  lg = log -p
  st = status
[color]
  branch = auto
  status = auto
  ui = true
[color "branch"]
  current = yellow black
  local = yellow
  remote = magenta
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red reverse
  new = green reverse
  whitespace = white reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan reverse
  branch = magenta
[core]
  editor = mate -w
  quotepath = false
[user]
  email = ckulakowski@gmail.com
  name = Caitlyn Kulakowski
[credential]
  helper = osxkeychain
